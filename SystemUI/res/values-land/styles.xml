<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="BrightnessDialogContainer" parent="@style/BaseBrightnessDialogContainer">
        <item name="android:layout_width">360.0dip</item>
    </style>
    <style name="DockedDividerBackground">
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_width">@dimen/mw_background_thickness</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="DockedDividerFocusedFrame">
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_width">@dimen/mw_focused_frame_thickness</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="DockedDividerHandle">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">48.0dip</item>
        <item name="android:layout_height">96.0dip</item>
    </style>
    <style name="DockedDividerMinimizedShadow">
        <item name="android:layout_width">8.0dip</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="SideScreenDividerHandler">
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_width">@dimen/side_screen_divider_handler_long</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_marginTop">@dimen/side_screen_divider_handler_short</item>
    </style>
    <style name="SideScreenDividerHandlerView">
        <item name="android:layout_width">@dimen/side_screen_divider_handler_long</item>
        <item name="android:layout_height">@dimen/side_screen_divider_handler_short</item>
    </style>
    <style name="SideScreenDividerShadow">
        <item name="android:layout_marginTop">@android:dimen/side_screen_divider_thickness_half</item>
    </style>
    <style name="Widget.Design.TabLayout" parent="@style/Base.Widget.Design.TabLayout">
        <item name="tabGravity">center</item>
        <item name="tabMode">fixed</item>
    </style>
</resources>
