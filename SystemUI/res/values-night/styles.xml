<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="BasicDescriptionStyle" parent="@android:style/Theme.DeviceDefault">
        <item name="android:background">@color/sesl_round_and_bgcolor_light</item>
    </style>
    <style name="PackageListActionbarStyle" parent="@android:style/Widget.DeviceDefault.Light.ActionBar">
        <item name="android:background">@color/sesl_round_and_bgcolor_light</item>
        <item name="android:displayOptions">homeAsUp|showTitle</item>
        <item name="android:titleTextStyle">@style/PackageListActionbarTitleText</item>
    </style>
    <style name="PackageListActionbarTitleText">
        <item name="android:textSize">19.0sp</item>
        <item name="android:textColor">@color/white_1</item>
        <item name="android:fontFamily">sec-roboto-regular</item>
    </style>
    <style name="PackageListActivityStyle" parent="@android:style/Theme.DeviceDefault">
        <item name="android:actionBarStyle">@style/PackageListActionbarStyle</item>
        <item name="android:statusBarColor">@color/sesl_round_and_bgcolor_light</item>
        <item name="android:navigationBarColor">@color/sesl_round_and_bgcolor_light</item>
    </style>
    <style name="Theme.AppCompat.DayNight" parent="@style/Theme.AppCompat" />
    <style name="Theme.AppCompat.DayNight.DarkActionBar" parent="@style/Theme.AppCompat" />
    <style name="Theme.AppCompat.DayNight.Dialog" parent="@style/Theme.AppCompat.Dialog" />
    <style name="Theme.AppCompat.DayNight.Dialog.Alert" parent="@style/Theme.AppCompat.Dialog.Alert" />
    <style name="Theme.AppCompat.DayNight.Dialog.MinWidth" parent="@style/Theme.AppCompat.Dialog.MinWidth" />
    <style name="Theme.AppCompat.DayNight.DialogWhenLarge" parent="@style/Theme.AppCompat.DialogWhenLarge" />
    <style name="Theme.AppCompat.DayNight.NoActionBar" parent="@style/Theme.AppCompat.NoActionBar" />
    <style name="Theme.DeviceDefault.QS.Detail.SeekBar" parent="@android:style/Theme.DeviceDefault" />
    <style name="Theme.DeviceDefault.VolumePopup.SeekBar" parent="@android:style/Theme.DeviceDefault" />
    <style name="Theme.SystemUI.Dialog" parent="@android:style/Theme.DeviceDefault.Dialog">
        <item name="android:windowMinWidthMajor">@android:dimen/dialog_min_width_major</item>
        <item name="android:windowMinWidthMinor">@android:dimen/dialog_min_width_minor</item>
    </style>
    <style name="Theme.SystemUI.Dialog.Alert" parent="@android:style/Theme.DeviceDefault.Dialog.Alert">
        <item name="android:windowMinWidthMajor">@android:dimen/dialog_min_width_major</item>
        <item name="android:windowMinWidthMinor">@android:dimen/dialog_min_width_minor</item>
    </style>
    <style name="Theme.SystemUI.Popup" parent="@android:style/Theme.DeviceDefault" />
    <style name="dialogTextStyle" parent="@android:style/TextAppearance">
        <item name="android:textColor">#ffe5e5e5</item>
    </style>
    <style name="dialogTheme" parent="@android:style/Theme.DeviceDefault.Dialog" />
</resources>
